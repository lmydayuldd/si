package com.sidc.rcu.hmi.bean;

import java.io.Serializable;

/**
 * @author J
 *
 */
public class LangBean implements Serializable {
	private static final long serialVersionUID = 2125711759359735328L;
	private String name;
	private String lange;

	public String getName() {
		return name;
	}

	public String getLange() {
		return lange;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("LangBean [name=");
		builder.append(name);
		builder.append(", lange=");
		builder.append(lange);
		builder.append("]");
		return builder.toString();
	}

	public LangBean(String name, String lange) {
		super();
		this.name = name;
		this.lange = lange;
	}

}
