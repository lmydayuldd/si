package com.sidc.blackcore.api.mobile.guest.request;

import java.io.Serializable;

public class GuestInfoRequest implements Serializable {
	private static final long serialVersionUID = 8391352575371758591L;
	private String token;
	private String roomno;

	public String getToken() {
		return token;
	}

	public String getRoomno() {
		return roomno;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("GuestInfoRequest [token=");
		builder.append(token);
		builder.append(", roomno=");
		builder.append(roomno);
		builder.append("]");
		return builder.toString();
	}

	public GuestInfoRequest(String token, String roomno) {
		super();
		this.token = token;
		this.roomno = roomno;
	}

}
