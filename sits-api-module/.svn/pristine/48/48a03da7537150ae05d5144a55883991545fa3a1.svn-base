package com.sidc.blackcore.api.sits.shop.bean;

import java.io.Serializable;

public class ShopOrderLineBean implements Serializable {
	private static final long serialVersionUID = 3953439531834366095L;
	private int categoryid;
	private String categoryname;
	private int vendoryid;
	private String vendorname;
	private int itemid;
	private String itemname;
	private float amount;
	private int qty;

	public int getCategoryid() {
		return categoryid;
	}

	public String getCategoryname() {
		return categoryname;
	}

	public int getVendoryid() {
		return vendoryid;
	}

	public String getVendorname() {
		return vendorname;
	}

	public int getItemid() {
		return itemid;
	}

	public String getItemname() {
		return itemname;
	}

	public float getAmount() {
		return amount;
	}

	public int getQty() {
		return qty;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ShopOrderLineBean [categoryid=");
		builder.append(categoryid);
		builder.append(", categoryname=");
		builder.append(categoryname);
		builder.append(", vendoryid=");
		builder.append(vendoryid);
		builder.append(", vendorname=");
		builder.append(vendorname);
		builder.append(", itemid=");
		builder.append(itemid);
		builder.append(", itemname=");
		builder.append(itemname);
		builder.append(", amount=");
		builder.append(amount);
		builder.append(", qty=");
		builder.append(qty);
		builder.append("]");
		return builder.toString();
	}

	public ShopOrderLineBean(int categoryid, String categoryname, int vendoryid, String vendorname, int itemid,
			String itemname, float amount, int qty) {
		super();
		this.categoryid = categoryid;
		this.categoryname = categoryname;
		this.vendoryid = vendoryid;
		this.vendorname = vendorname;
		this.itemid = itemid;
		this.itemname = itemname;
		this.amount = amount;
		this.qty = qty;
	}

}
