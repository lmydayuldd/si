package com.sidc.blackcore.api.sits.shop.bean;

import java.io.Serializable;

public class ShopOrderItemBean implements Serializable {
	private static final long serialVersionUID = 2529036652993777384L;
	private int itemid;
	private int qty;

	public int getItemid() {
		return itemid;
	}

	public int getQty() {
		return qty;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ShopOrderItemBean [itemid=");
		builder.append(itemid);
		builder.append(", qty=");
		builder.append(qty);
		builder.append("]");
		return builder.toString();
	}

	public ShopOrderItemBean(int itemid, int qty) {
		super();
		this.itemid = itemid;
		this.qty = qty;
	}

}
