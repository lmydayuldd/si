package com.sidc.blackcore.api.ra.rcumodesetting.request;

public class RcuGroupModeRequest implements java.io.Serializable {

	private static final long serialVersionUID = -8722476276343028566L;
	private int modeid;
	private int groupid;

	public int getModeid() {
		return modeid;
	}

	public int getGroupid() {
		return groupid;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("RcuModeDeviceSetRequest [modeid=");
		builder.append(modeid);
		builder.append(", groupid=");
		builder.append(groupid);
		builder.append("]");
		return builder.toString();
	}

	public RcuGroupModeRequest(int modeid, int groupid) {
		super();
		this.modeid = modeid;
		this.groupid = groupid;
	}

}
