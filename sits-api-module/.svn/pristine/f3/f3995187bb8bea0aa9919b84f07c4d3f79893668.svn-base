package com.sidc.blackcore.api.mobile.laundry.bean;

import java.io.Serializable;

public class LaundryClassBean implements Serializable {
	private static final long serialVersionUID = -2803537752212878201L;
	private int id;
	private int status;
	private float servicecharge;

	public int getId() {
		return id;
	}

	public int getStatus() {
		return status;
	}

	public float getServicecharge() {
		return servicecharge;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("LaundryClassBean [id=");
		builder.append(id);
		builder.append(", status=");
		builder.append(status);
		builder.append(", servicecharge=");
		builder.append(servicecharge);
		builder.append("]");
		return builder.toString();
	}

	public LaundryClassBean(int id, int status, float servicecharge) {
		super();
		this.id = id;
		this.status = status;
		this.servicecharge = servicecharge;
	}

}
