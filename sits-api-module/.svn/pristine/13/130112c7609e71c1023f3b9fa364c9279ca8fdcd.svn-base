package com.sidc.blackcore.api.ra.rcugroupdevice.bean;

public class RcuGroupDeviceGroupInfoBean implements java.io.Serializable {
	private static final long serialVersionUID = -6599249668237163870L;
	private int deviceid;
	private int groupid;
	private String groupname;

	public int getDeviceid() {
		return deviceid;
	}

	public int getGroupid() {
		return groupid;
	}

	public String getGroupname() {
		return groupname;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("RcuGroupDeviceGroupInfoBean [deviceid=");
		builder.append(deviceid);
		builder.append(", groupid=");
		builder.append(groupid);
		builder.append(", groupname=");
		builder.append(groupname);
		builder.append("]");
		return builder.toString();
	}

	public RcuGroupDeviceGroupInfoBean(int deviceid, int groupid, String groupname) {
		super();
		this.deviceid = deviceid;
		this.groupid = groupid;
		this.groupname = groupname;
	}

}
