package com.sidc.blackcore.api.sits.account.request;

import java.io.Serializable;

/**
 * @author Joe
 *
 */
public class SystemUserLogoutRequest implements Serializable {

	private static final long serialVersionUID = -7640589175172191589L;

	private String user;

	public String getUser() {
		return user;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("SystemUserLogoutRequest [user=");
		builder.append(user);
		builder.append("]");
		return builder.toString();
	}

	public SystemUserLogoutRequest(String user) {
		super();
		this.user = user;
	}

}
