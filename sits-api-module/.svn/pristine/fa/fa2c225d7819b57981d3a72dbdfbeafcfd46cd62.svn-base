package com.sidc.blackcore.api.sits.token.bean;

import java.io.Serializable;

public class TokenInfoBean implements Serializable {
	private static final long serialVersionUID = 7896935182890632819L;
	private String id;
	private int type;

	public String getId() {
		return id;
	}

	public int getType() {
		return type;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("TokenInfoBean [id=");
		builder.append(id);
		builder.append(", type=");
		builder.append(type);
		builder.append("]");
		return builder.toString();
	}

	public TokenInfoBean(String id, int type) {
		super();
		this.id = id;
		this.type = type;
	}

}
