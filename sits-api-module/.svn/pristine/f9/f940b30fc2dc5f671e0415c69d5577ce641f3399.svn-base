package com.sidc.blackcore.api.sits.hotelinfo.response;

import java.io.Serializable;

/**
 * @author Joe
 *
 */
public class HotelInfoResponse implements Serializable {
	private static final long serialVersionUID = 8359612217005469611L;
	private String hotelcode;
	private String hotelname;

	public String getHotelcode() {
		return hotelcode;
	}

	public String getHotelname() {
		return hotelname;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("HotelInfoResponse [hotelcode=");
		builder.append(hotelcode);
		builder.append(", hotelname=");
		builder.append(hotelname);
		builder.append("]");
		return builder.toString();
	}

	public HotelInfoResponse(String hotelcode, String hotelname) {
		super();
		this.hotelcode = hotelcode;
		this.hotelname = hotelname;
	}

}
