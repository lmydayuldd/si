package com.sidc.blackcore.api.sits.langs.bean;

import java.io.Serializable;

public class LangsBean implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = -6567731403384008703L;
	private String lange;
	private String name;

	public String getLange() {
		return lange;
	}

	public String getName() {
		return name;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("LangsBean [lange=");
		builder.append(lange);
		builder.append(", name=");
		builder.append(name);
		builder.append("]");
		return builder.toString();
	}

	public LangsBean(String lange, String name) {
		super();
		this.lange = lange;
		this.name = name;
	}

}
