package com.sidc.blackcore.api.mobile.activity.bean;

import java.io.Serializable;

public class ActivityFeeLangBean implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = -1462746925128000299L;
	private String langcode;
	private String name;

	public String getLangcode() {
		return langcode;
	}

	public String getName() {
		return name;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ActivityFeeLangBean [langcode=");
		builder.append(langcode);
		builder.append(", name=");
		builder.append(name);
		builder.append("]");
		return builder.toString();
	}

	public ActivityFeeLangBean(String langcode, String name) {
		super();
		this.langcode = langcode;
		this.name = name;
	}

}
