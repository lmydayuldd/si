package com.sidc.blackcore.api.mobile.message.bean;

import java.io.Serializable;

public class ChatMettingMessageBean implements Serializable {
	private static final long serialVersionUID = -6820917964810046212L;
	private String messageid;
	private String sendername;
	private int sendertype;
	private String receivername;
	private int receivertype;
	private String content;
	private String creationtime;

	public String getMessageid() {
		return messageid;
	}

	public String getSendername() {
		return sendername;
	}

	public int getSendertype() {
		return sendertype;
	}

	public String getReceivername() {
		return receivername;
	}

	public int getReceivertype() {
		return receivertype;
	}

	public String getContent() {
		return content;
	}

	public String getCreationtime() {
		return creationtime;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ChatMettingMessageBean [messageid=");
		builder.append(messageid);
		builder.append(", sendername=");
		builder.append(sendername);
		builder.append(", sendertype=");
		builder.append(sendertype);
		builder.append(", receivername=");
		builder.append(receivername);
		builder.append(", receivertype=");
		builder.append(receivertype);
		builder.append(", content=");
		builder.append(content);
		builder.append(", creationtime=");
		builder.append(creationtime);
		builder.append("]");
		return builder.toString();
	}

	public ChatMettingMessageBean(String messageid, String sendername, int sendertype, String receivername,
			int receivertype, String content, String creationtime) {
		super();
		this.messageid = messageid;
		this.sendername = sendername;
		this.sendertype = sendertype;
		this.receivername = receivername;
		this.receivertype = receivertype;
		this.content = content;
		this.creationtime = creationtime;
	}

}
