package com.sidc.quartz.api.response;

import java.io.Serializable;

public class ScheduleStatusResponse implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 8374120191753323189L;
	private boolean enabled;
	
	public ScheduleStatusResponse(boolean enabled) {
		super();
		this.enabled = enabled;
	}
	
	public boolean isEnabled() {
		return enabled;
	}
	
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ScheduleStatusResponse [enabled=");
		builder.append(enabled);
		builder.append("]");
		return builder.toString();
	}
	
}
