package com.sidc.blackcore.api.mobile.laundry.bean;

import java.io.Serializable;

public class LaundryOrderItemBean implements Serializable {
	private static final long serialVersionUID = -6932967619764183429L;
	private int itemid;
	private int washtypeid;
	private int returntypeid;
	private int piece;

	public int getItemid() {
		return itemid;
	}

	public int getWashtypeid() {
		return washtypeid;
	}

	public int getReturntypeid() {
		return returntypeid;
	}

	public int getPiece() {
		return piece;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("LaundryOrderItemBean [itemid=");
		builder.append(itemid);
		builder.append(", washtypeid=");
		builder.append(washtypeid);
		builder.append(", returntypeid=");
		builder.append(returntypeid);
		builder.append(", piece=");
		builder.append(piece);
		builder.append("]");
		return builder.toString();
	}

	public LaundryOrderItemBean(int itemid, int washtypeid, int returntypeid, int piece) {
		super();
		this.itemid = itemid;
		this.washtypeid = washtypeid;
		this.returntypeid = returntypeid;
		this.piece = piece;
	}

}
