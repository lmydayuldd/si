package com.sidc.blackcore.api.ra.rcumodesetting.response;

import java.io.Serializable;

public class RcuModeDeviceResponse implements Serializable {
	private static final long serialVersionUID = -54910268844274298L;

	private int modeId;
	private int rcuDeviceId;
	private String device;

	public int getModeId() {
		return modeId;
	}

	public int getRcuDeviceId() {
		return rcuDeviceId;
	}

	public String getDevice() {
		return device;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("RcuModeDeviceResponse [modeId=");
		builder.append(modeId);
		builder.append(", rcuDeviceId=");
		builder.append(rcuDeviceId);
		builder.append(", device=");
		builder.append(device);
		builder.append("]");
		return builder.toString();
	}

	public RcuModeDeviceResponse(int modeId, int rcuDeviceId, String device) {
		super();
		this.modeId = modeId;
		this.rcuDeviceId = rcuDeviceId;
		this.device = device;
	}

}
