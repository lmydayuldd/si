package com.sidc.blackcore.api.mobile.activity.bean;

import java.io.Serializable;
import java.util.Arrays;

public class ActivityRepeatFrequentBean implements Serializable {
	private static final long serialVersionUID = 4021920378651539464L;
	private String[] days;

	public String[] getDays() {
		return days;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ActivityRepeatFrequentBean [days=");
		builder.append(Arrays.toString(days));
		builder.append("]");
		return builder.toString();
	}

	public ActivityRepeatFrequentBean(String[] days) {
		super();
		this.days = days;
	}

}
