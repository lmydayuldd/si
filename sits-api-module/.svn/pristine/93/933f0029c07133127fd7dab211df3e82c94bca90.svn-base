package com.sidc.blackcore.api.ra.response;

import java.io.Serializable;

/**
 * @author Nandin
 *
 */
public class RCUStatusBean implements Serializable {

	private static final long serialVersionUID = 3814949423675613229L;
	private int status;

	public RCUStatusBean(int status) {
		super();
		this.status = status;
	}

	public int getStatus() {
		return status;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("RCUStatus [\n\tstatus=");
		builder.append(status);
		builder.append("]");
		return builder.toString();
	}
	
}
