package com.sidc.blackcore.api.mobile.activity.bean;

import java.io.Serializable;

public class ActivityRepeatInfoBean implements Serializable {
	private static final long serialVersionUID = 682407695256346459L;
	private int repeatid;
	private String starttime;
	private String endtime;
	private String description;

	public int getRepeatid() {
		return repeatid;
	}

	public String getStarttime() {
		return starttime;
	}

	public String getEndtime() {
		return endtime;
	}

	public String getDescription() {
		return description;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ActivityRepeatInfoBean [repeatid=");
		builder.append(repeatid);
		builder.append(", starttime=");
		builder.append(starttime);
		builder.append(", endtime=");
		builder.append(endtime);
		builder.append(", description=");
		builder.append(description);
		builder.append("]");
		return builder.toString();
	}

	public ActivityRepeatInfoBean(int repeatid, String starttime, String endtime, String description) {
		super();
		this.repeatid = repeatid;
		this.starttime = starttime;
		this.endtime = endtime;
		this.description = description;
	}

}
