package com.sidc.raudp.bean;

import java.io.Serializable;

/**
 * @author Nandin
 *
 */
public class BlubDeviceBean implements Serializable {

	private static final long serialVersionUID = -1735271108898428380L;
	private String name;
	private int position;
	private int circuit;

	public BlubDeviceBean(String name, int position, int circuit) {
		super();
		this.name = name;
		this.position = position;
		this.circuit = circuit;
	}

	public String getName() {
		return name;
	}

	public int getPosition() {
		return position;
	}

	public int getCircuit() {
		return circuit;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("BlubDeviceBean [\n\tname=");
		builder.append(name);
		builder.append(", position=");
		builder.append(position);
		builder.append(", circuit=");
		builder.append(circuit);
		builder.append("]");
		return builder.toString();
	}

}
