package com.sidc.blackcore.api.sits.room.request;

import java.io.Serializable;

public class RoomCheckInInfoRequest implements Serializable {
	private static final long serialVersionUID = 6216133194225824009L;
	private String token;

	public String getToken() {
		return token;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("RoomCheckInInfoRequest [token=");
		builder.append(token);
		builder.append("]");
		return builder.toString();
	}

	public RoomCheckInInfoRequest(String token) {
		super();
		this.token = token;
	}

}
