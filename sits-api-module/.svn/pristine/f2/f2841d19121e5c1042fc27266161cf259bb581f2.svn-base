package com.sidc.blackcore.api.sits.shop.response;

import java.io.Serializable;

/**
 * 
 * @author Allen Huang
 *
 */
public class ShoppingPriceResponse implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2506603749956523727L;
	private float price;
	
	public ShoppingPriceResponse(float price) {
		super();
		this.price = price;
	}

	public float getPrice() {
		return price;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ShoppingPriceResponse [price=");
		builder.append(price);
		builder.append("]");
		return builder.toString();
	}
	
}
