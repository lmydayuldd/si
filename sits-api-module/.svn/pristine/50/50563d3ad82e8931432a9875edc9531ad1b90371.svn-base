package com.sidc.blackcore.api.sits.shop.bean;

import java.io.Serializable;

public class ShopItemBean implements Serializable {
	private static final long serialVersionUID = -3632316034038245839L;
	private int itemid;
	private int categoryid;
	private int vendoryid;
	private float sellingPrice;
	private int qty;

	public int getItemid() {
		return itemid;
	}

	public int getCategoryid() {
		return categoryid;
	}

	public int getVendoryid() {
		return vendoryid;
	}

	public float getSellingPrice() {
		return sellingPrice;
	}

	public int getQty() {
		return qty;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ShopItemBean [itemid=");
		builder.append(itemid);
		builder.append(", categoryid=");
		builder.append(categoryid);
		builder.append(", vendoryid=");
		builder.append(vendoryid);
		builder.append(", sellingPrice=");
		builder.append(sellingPrice);
		builder.append(", qty=");
		builder.append(qty);
		builder.append("]");
		return builder.toString();
	}

	public ShopItemBean(int itemid, int categoryid, int vendoryid, float sellingPrice, int qty) {
		super();
		this.itemid = itemid;
		this.categoryid = categoryid;
		this.vendoryid = vendoryid;
		this.sellingPrice = sellingPrice;
		this.qty = qty;
	}

}
