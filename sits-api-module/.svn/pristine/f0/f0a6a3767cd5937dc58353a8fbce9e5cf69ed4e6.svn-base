package com.derex.cm.stb.api.request;

import java.io.Serializable;

/**
 * @author Nandin
 *
 */
public class StbRebootRequest implements Serializable {

	private static final long serialVersionUID = -3397430981431668243L;
	private String stbip;

	public StbRebootRequest(String stbip) {
		super();
		this.stbip = stbip;
	}

	public String getStbip() {
		return stbip;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("StbRebootRequest [stbip=\n");
		builder.append(stbip);
		builder.append("]");
		return builder.toString();
	}

}
