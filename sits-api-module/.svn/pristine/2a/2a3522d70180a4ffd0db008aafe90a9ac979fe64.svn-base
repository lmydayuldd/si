package com.sidc.blackcore.api.sits.roomservice.bean;

import java.io.Serializable;

public class RoomServiceOrderSetItemBean implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 7642563121657542033L;
	private int itemid;

	public int getItemid() {
		return itemid;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("RoomServiceOrderSetItemBean [itemid=");
		builder.append(itemid);
		builder.append("]");
		return builder.toString();
	}

	public RoomServiceOrderSetItemBean(int itemid) {
		super();
		this.itemid = itemid;
	}

}
